#Leap Year Calculator

print("What Year would you like to investigate?")

year = int(input())

if year % 4 == 0:

  if year % 100 == 0:
    print(f"{year} is not a leap year")

  elif year % 400 == 0:
    print(f"{year} is a leap year")

  else:
    print(f"{year} is a leap year")

else:
  print(f"{year} is not a leap year")
